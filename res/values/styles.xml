<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="NoTitleBarTheme" parent="android:Theme">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:textColor">#333333</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="noAnimation">
        <item name="android:activityOpenEnterAnimation">@null</item>
        <item name="android:activityOpenExitAnimation">@null</item>
        <item name="android:activityCloseEnterAnimation">@null</item>
        <item name="android:activityCloseExitAnimation">@null</item>
        <item name="android:taskOpenEnterAnimation">@null</item>
        <item name="android:taskOpenExitAnimation">@null</item>
        <item name="android:taskCloseEnterAnimation">@null</item>
        <item name="android:taskCloseExitAnimation">@null</item>
        <item name="android:taskToFrontEnterAnimation">@null</item>
        <item name="android:taskToFrontExitAnimation">@null</item>
        <item name="android:taskToBackEnterAnimation">@null</item>
        <item name="android:taskToBackExitAnimation">@null</item>
    </style>

    <style name="TitleBar">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50dip</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">22sp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/titlebar_bg</item>
    </style>

    <style name="TitleBarRun">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50dip</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/titlebar_bg</item>
    </style>

    <style name="titleBtnAction">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">48dip</item>
        <item name="android:background">@drawable/titlebar_press</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="DialogStyle">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="useDesc">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">50dip</item>
        <item name="android:padding">10dip</item>
        <item name="android:textColor">#232d50</item>
        <item name="android:textSize">@dimen/descSize</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:lineSpacingExtra">3dip</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:background">@color/gray2</item>
    </style>

    <style name="btnConfirmStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">30dip</item>
        <item name="android:paddingRight">30dip</item>
        <item name="android:paddingTop">10dip</item>
        <item name="android:paddingBottom">10dip</item>
        <item name="android:background">@color/btnBg</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
    </style>

    <!-- layout布局Width和Height都是 FILL_PARENT -->
    <style name="layoutAllFill">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <!-- layout布局Width是FILL_PARENT,Height是100dip -->
    <style name="layoutWFH40">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">46dip</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">22sp</item>
    </style>

    <style name="LayoutOutside">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:padding">0dip</item>
        <item name="android:textColor">@color/gray_deep</item>
        <item name="android:background">@color/gray_light</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="RelativeInside">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:textColor">@color/gray_deep</item>
    </style>

    <style name="ListItemOutside">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:padding">0dip</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="ProgressDialogCustom" parent="@android:style/Theme.Dialog">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">14sp</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>

    <style name="DialogOutside">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:padding">0dip</item>
        <item name="android:background">@color/gray_light</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="LoadingOutside">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:padding">0dip</item>
        <item name="android:background">@color/dialogBg</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="prefs">
        <item name="android:windowBackground">@color/gray_light</item>
        <item name="android:focusable">false</item>
        <item name="android:textColor">@color/gray_deep</item>
        <item name="android:cacheColorHint">#cccccc</item>
        <item name="android:windowNoTitle">false</item>
    </style>

    <style name="MyDialogStyle">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="main_tab">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#333</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">@drawable/home_btn_bg</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:button">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:drawablePadding">0dip</item>
        <item name="android:paddingTop">5dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="th">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:padding">2dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_margin">0.5dip</item>
        <item name="android:background">@color/blue_light</item>
    </style>

    <style name="td">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:padding">2dip</item>
        <item name="android:layout_margin">0.5dip</item>
        <item name="android:background">@color/gray_light</item>
    </style>

    <style name="tdRowspanFirst">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:padding">2dip</item>
        <item name="android:layout_marginTop">0.5dip</item>
        <item name="android:layout_marginBottom">0dip</item>
        <item name="android:layout_marginLeft">0.5dip</item>
        <item name="android:layout_marginRight">0.5dip</item>
        <item name="android:background">@color/gray_light</item>
    </style>

    <style name="tdRowspan">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:padding">2dip</item>
        <item name="android:layout_marginTop">0dip</item>
        <item name="android:layout_marginBottom">0dip</item>
        <item name="android:layout_marginLeft">0.5dip</item>
        <item name="android:layout_marginRight">0.5dip</item>
        <item name="android:background">@color/gray_light</item>
    </style>

    <style name="tdRowspanLast">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:padding">2dip</item>
        <item name="android:layout_marginTop">0dip</item>
        <item name="android:layout_marginBottom">0.5dip</item>
        <item name="android:layout_marginLeft">0.5dip</item>
        <item name="android:layout_marginRight">0.5dip</item>
        <item name="android:background">@color/gray_light</item>
    </style>

    <style name="MyRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/food_rating_bar_full</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:isIndicator">true</item>
        <item name="android:minHeight">15dip</item>
        <item name="android:maxHeight">15dip</item>
        <item name="android:background">@color/gray_light</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="tab_rb_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:textColor">@color/tab_selector_tv_color</item>
        <item name="android:gravity">center_horizontal|bottom</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:textSize">12dip</item>
        <item name="android:background">@drawable/tab_selector_checked_bg</item>
    </style>

    <style name="FormHor">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">20dip</item>
        <item name="android:paddingRight">20dip</item>
        <item name="android:paddingTop">5dip</item>
        <item name="android:textSize">@dimen/descSize</item>
    </style>

    <style name="FormHorPadding0">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:textSize">@dimen/descSize</item>
    </style>

    <style name="FormVert">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingLeft">20dip</item>
        <item name="android:paddingRight">20dip</item>
        <item name="android:paddingTop">5dip</item>
        <item name="android:textSize">@dimen/descSize</item>
    </style>

    <style name="EditForm3">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">5dip</item>
        <item name="android:paddingRight">5dip</item>
        <item name="android:textSize">@dimen/descSize</item>
    </style>

    <style name="BtnLayout">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">15dip</item>
        <item name="android:paddingRight">15dip</item>
        <item name="android:paddingTop">5dip</item>
        <item name="android:textSize">@dimen/descSize</item>
    </style>

    <style name="ListBtnStyle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">48dip</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">15dip</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:textSize">@dimen/descSize</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:clickable">true</item>
    </style>

</resources>